    .theme-toggle .slider {
      position: absolute;
      cursor: pointer;
      inset: 0;
      background-color: #bbb;
      transition: 0.3s;
      border-radius: 30px;
    }

    .theme-toggle .slider::before {
      content: "";
      position: absolute;
      width: 22px;
      height: 22px;
      left: 3px;
      top: 3px;
      background-color: #fff;
      border-radius: 50%;
      transition: 0.3s;
    }

    .theme-toggle input:checked + .slider {
      background-color: #3b82f6;
    }

    .theme-toggle input:checked + .slider::before {
      transform: translateX(32px);
    }

